pipeline { 
    agent any 
    stages { 
        stage("Checkout /Clone from Git & Maven Build") { 
            steps { 
                      git branch: "Main" ,   url : "https://github.com/devopsusergit/PetStoreWebApp.git"
                      sh  'mvn clean'
                      sh  ' mvn package'
            } 
        }
stage('Parallel Stage') {      parallel {        stage('Job 1') {          steps {            echo 'Running Job 1'          }        }        stage('Job 2') {          steps {            echo 'Running Job 2'          }        }








}
}


